<?xml version='1.0' encoding='utf-8'?>
<widget id="com.matchpan.main" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Matchpan</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon background="www/img/matchpanLogo.png" density="ldpi" foreground="www/img/matchpanLogo.png" />
    <icon background="www/img/matchpanLogo.png" density="mdpi" foreground="www/img/matchpanLogo.png" />
    <icon background="www/img/matchpanLogo.png" density="hdpi" foreground="www/img/matchpanLogo.png" />
    <icon background="www/img/matchpanLogo.png" density="xhdpi" foreground="www/img/matchpanLogo.png" />
    <icon background="www/img/matchpanLogo.png" density="xxhdpi" foreground="www/img/matchpanLogo.png" />
    <icon background="www/img/matchpanLogo.png" density="xxxhdpi" foreground="www/img/matchpanLogo.png" />
</widget>
